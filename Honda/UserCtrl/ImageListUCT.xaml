<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Honda.UserCtrl" x:Class="Honda.UserCtrl.ImageListUCT"
    mc:Ignorable="d"
    d:DesignHeight="138" d:DesignWidth="300">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <StackPanel x:Name="spImagelist" Orientation="Horizontal" />
            <local:ImageButtonEx x:Name="btnAdd" Content="ImageButtonEx" Style="{DynamicResource ImageButtonExStyle3}"
                                 Width="177" ImagePressSource="/Honda;component/Assets/Remark/add_2Edit.png"
                                 ImageSource="/Honda;component/Assets/Remark/add_1Edit.png" Click="btnAdd_Click" />
        </StackPanel>
    </Grid>
</UserControl>